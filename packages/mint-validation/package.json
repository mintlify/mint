{
  "name": "@mintlify/mint-validation",
  "version": "0.0.4",
  "description": "Validates mint.json files",
  "main": "./bin/index.js",
  "types": "./bin/index.d.ts",
  "keywords": [
    "mintlify",
    "mint"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "publish-package": "npm run build && npm publish --access public"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "@/utils/(.*)": "<rootDir>/src/utils/$1",
      "@/schemas/(.*)": "<rootDir>/src/schemas/$1",
      "@/types/(.*)": "<rootDir>/src/types/$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mintlify/mint/packages/mint-validation"
  },
  "bugs": {
    "url": "https://github.com/mintlify/mint/issues"
  },
  "author": "Mintlify, Inc.",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "babel-jest": "^29.3.1",
    "jest": "^29.3.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "zod": "^3.19.1"
  }
}
